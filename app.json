{
	"expo": {
		"name": "Moose",
		"slug": "Moose",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/moose-day.png",
		"scheme": "moose",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#FFA500"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.jazzsolar.moose"
		},
		"android": {
			"edgeToEdgeEnabled": true,
			"adaptiveIcon": {
				"foregroundImage": "./assets/moose-day.png",
				"backgroundColor": "#FFA500"
			},
			"package": "com.jazzsolar.moose"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
